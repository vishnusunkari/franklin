<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.franklin.eac.model.pmp.PasswordResetTemp" table="PasswordResetTemp" lazy="false">
        <id name="passwordResetTempId" type="integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <timestamp name="auditStamp" column="audit_stamp"/>
        <property name="username" type="string" length="20"/>
		<property name="effectiveDate" type="date" column="effective_date" length="10"/>        		        	        
    </class>
</hibernate-mapping>

